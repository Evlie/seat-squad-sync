        {
            "type": "vcs",
            "url": "https://github.com/Evlie/seat-squad-sync.git"
        }
    ],
    "name": "cryptaeve/seat-squad-sync",
    "description": "Module sync seat squad members into role affiliations",
    "type": "seat-plugin",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Crypta Electrica",
            "email": "crypta@crypta.tech"
        }
    ],

    "minimum-stability": "stable",
    "autoload": {
